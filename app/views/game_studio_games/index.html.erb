<h1><%= @game_studio.studio_name %> Games</h1>

<% @games.each do |game| %>
	<h3>Game: <%= game.game_name %></h3>
  <p>Game cost: <%= game.game_cost %></p>
  <p>Game genre: <%= game.genre %></p>
  <p>Multiplatform?: <%= game.multiplatform %></p>
  <p>Created at: <%= game.created_at %></p>
  <p>Updated at: <%= game.updated_at %></p>
<% end %>